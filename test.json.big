 guest\nexchange = imp\n\n[agent]\npid-file = /run/imp/agent.pid\nlogfile = /var/log/imp/agent.log\nhostnames = mq-master.dreamaas.cs.kuleuven.be\ncode_dir = /var/lib/imp\nEOF\n\n# configure dns\ncat > /etc/resolv.conf <<EOF\nnameserver 8.8.8.8\nEOF\n\n# start supervisord\nsystemctl start imp-agent", "requires": [], "key_name": "bartvb", "image": "ed9ae349-9b80-4d13-b123-f4d6ffbf752d", "name": "mq-master.dreamaas.cs.kuleuven.be", "iaas_config": {"tenant": "bartvb", "url": "http://dnetcloud.cs.kuleuven.be:5000/v2.0", "password": "oW7eilei", "username": "bartvb"}, "id": "vm::Host[dnetcloud,name=mq-master.dreamaas.cs.kuleuven.be],v=1395053086", "flavor": "tiny", "version": 1395053086, "key_value": "ssh-rsa  bart@verdana.cs.kuleuven.be\n"}, {"purged": false, "requires": [], "permissions": 644.0, "path": "/etc/sysconfig/selinux", "owner": "root", "group": "root", "id": "std::File[postprocess-0.dnetcloud.dreamaas.cs.kuleuven.be,path=/etc/sysconfig/selinux],v=1395053086", "hash": "f8c1e58b9c78af4bfcfdba72faa6085bbab79cfa", "reload": false, "version": 1395053086}, {"purged": false, "requires": [], "permissions": 644.0, "path": "/usr/share/taskworker/workers.jar", "owner": "root", "group": "root", "id": "std::File[render-0.dnetcloud.dreamaas.cs.kuleuven.be,path=/usr/share/taskworker/workers.jar],v=1395053086", "hash": "77a3cf860f49c9967e65f1bb8761671b9e650134", "reload": false, "version": 1395053086}, {"purged": false, "requires": ["std::Directory[mon-master.dreamaas.cs.kuleuven.be,path=/etc/flens.d],v=1395053086"], "permissions": 644.0, "path": "/etc/flens.d/config.json", "owner": "root", "group": "root", "id": "std::File[mon-master.dreamaas.cs.kuleuven.be,path=/etc/flens.d/config.json],v=1395053086", "hash": "8bf0587130db6f86dea049408fd03654021196ad", "reload": true, "version": 1395053086}, {"id": "std::Package[preprocess-0.dnetcloud.dreamaas.cs.kuleuven.be,name=unzip],v=1395053086", "requires": ["std::File[preprocess-0.dnetcloud.dreamaas.cs.kuleuven.be,path=/etc/yum.repos